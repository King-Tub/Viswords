{
  "version": 2,
  "builds": [
    {
      "src": "Qic1/viswords/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.10"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "Qic1/viswords/wsgi.py"
    }
  ],
  "env": {
    "FLASK_ENV": "production",
    "PYTHONPATH": "./Qic1/viswords"
  }
}